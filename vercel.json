{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)", 
      "dest": "/client/assets/$1"
    },
    {
      "src": "/scan",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/(.*)", 
    }
  ]
}
