{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/scan", 
      "dest": "/client/server.html"
    },
    {
      "src": "/", 
      "dest": "/client/index.html"
    {
      "src": "/(.*)", 
      "dest": "index.js"
    }
  ]
}
